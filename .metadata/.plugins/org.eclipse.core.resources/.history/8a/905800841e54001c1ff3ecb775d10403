package com.te.practice;

public class LargestElementInArray {

	static int largestElement(int arr[] , int size) {
		
		
		return arr.length-1;
		
	}
	public static void main(String[] args) {
		
		for (int i = 0; i < size; i++) {
			for (int j = i+1 ; j < size; j++) {
				if(arr[i] == arr[j]) {
					int	temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
				}
			}
		}
	}
	
	
	

	
}
